      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2022; © 2022 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{5D40791B-D3A8-42CF-8C08-B195B81CA53E}"
          SQLTask:SqlStatementSource="-- Crear secuencia para ProductSK (si no existe)&#xA;IF OBJECT_ID('dw.SeqProductSK','SO') IS NULL&#xA;    CREATE SEQUENCE dw.SeqProductSK AS BIGINT START WITH 1 INCREMENT BY 1;&#xA;&#xA;-- Agregar default constraint para autogenerar SK (si no existe)&#xA;IF NOT EXISTS (&#xA;  SELECT 1&#xA;  FROM sys.default_constraints dc&#xA;  JOIN sys.columns c&#xA;    ON c.object_id = OBJECT_ID('dw.DimProductos') AND c.column_id = dc.parent_column_id&#xA;  WHERE c.name = 'ProductSK'&#xA;)&#xA;BEGIN&#xA;  ALTER TABLE dw.DimProductos&#xA;  ADD CONSTRAINT DF_DimProductos_SK&#xA;    DEFAULT NEXT VALUE FOR dw.SeqProductSK FOR ProductSK;&#xA;END&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
