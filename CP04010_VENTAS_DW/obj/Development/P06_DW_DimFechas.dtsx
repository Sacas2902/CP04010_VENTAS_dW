      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{1E09F924-02CE-4E7F-ACF4-D9CD1FDC868B}"
          SQLTask:SqlStatementSource="-- Parámetros del paquete&#xA;DECLARE @StartDate DATE = ?;&#xA;DECLARE @EndDate   DATE = ?;&#xA;&#xA;;WITH d AS (&#xA;  SELECT @StartDate AS [Date]&#xA;  UNION ALL&#xA;  SELECT DATEADD(DAY,1,[Date]) FROM d WHERE [Date] &lt; @EndDate&#xA;)&#xA;INSERT INTO dw.DimFechas(&#xA;  DateKey,[Date],[Year],[Quarter],[Month],MonthName,[Day],DayOfWeek,DayName,IsWeekend&#xA;)&#xA;SELECT&#xA;  CONVERT(INT,FORMAT([Date],'yyyyMMdd')) AS DateKey,&#xA;  [Date]                                         AS [Date],&#xA;  DATEPART(YEAR,[Date])                          AS [Year],&#xA;  DATEPART(QUARTER,[Date])                       AS [Quarter],&#xA;  DATEPART(MONTH,[Date])                         AS [Month],&#xA;  DATENAME(MONTH,[Date])                         AS MonthName,&#xA;  DATEPART(DAY,[Date])                           AS [Day],&#xA;  DATEPART(WEEKDAY,[Date])                       AS DayOfWeek,&#xA;  DATENAME(WEEKDAY,[Date])                       AS DayName,&#xA;  CASE WHEN DATENAME(WEEKDAY,[Date]) IN (N'Saturday',N'Sunday') THEN 1 ELSE 0 END AS IsWeekend&#xA;FROM d&#xA;WHERE NOT EXISTS (&#xA;  SELECT 1 FROM dw.DimFechas f&#xA;  WHERE f.DateKey = CONVERT(INT,FORMAT(d.[Date],'yyyyMMdd'))&#xA;)&#xA;OPTION (MAXRECURSION 0);&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
          <SQLTask:ParameterBinding
            SQLTask:ParameterName="0"
            SQLTask:DtsVariableName="User::StartDate"
            SQLTask:ParameterDirection="Input"
            SQLTask:DataType="135"
            SQLTask:ParameterSize="-1" />
          <SQLTask:ParameterBinding
            SQLTask:ParameterName="1"
            SQLTask:DtsVariableName="User::EndDate"
            SQLTask:ParameterDirection="Input"
            SQLTask:DataType="135"
            SQLTask:ParameterSize="-1" />
        </SQLTask:SqlTaskData>
