      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{1C3352B5-730E-4A0C-90A5-7E1090D78925}"
          SQLTask:SqlStatementSource="/* Insertar nuevos pedidos en la dimensión degenerada */&#xA;INSERT INTO dw.DimPedidos (OrderNumber)&#xA;SELECT DISTINCT s.OrderNumber&#xA;FROM stg.Ventas s&#xA;WHERE NOT EXISTS (&#xA;  SELECT 1 FROM dw.DimPedidos d WHERE d.OrderNumber = s.OrderNumber&#xA;);&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
      DTS:refId="Package\Cargar_HechosVentas"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Cargar_HechosVentas"
      DTS:DTSID="{76577317-83c1-4fe9-8d03-cbe0c9669ecc}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Cargar_HechosVentas"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2022; © 2022 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{1C3352B5-730E-4A0C-90A5-7E1090D78925}"
          SQLTask:SqlStatementSource="/* ===== Carga de hechos sin NULLs en FKs ===== */&#xA;&#xA;DECLARE @PromotionSK     BIGINT = (SELECT TOP 1 PromotionSK   FROM dw.DimPromociones   WHERE PromotionCode = N'SIN PROMOCION' AND IsCurrent = 1);&#xA;DECLARE @StateUnknownSK  BIGINT = (SELECT TOP 1 StateSK       FROM dw.DimEstados       WHERE StateID       = -1               AND IsCurrent = 1);&#xA;DECLARE @OfficeUnknownSK BIGINT = (SELECT TOP 1 SalesOfficeSK FROM dw.DimOficinasVentas WHERE SalesOfficeID = -1               AND IsCurrent = 1);&#xA;&#xA;INSERT INTO dw.HechosVentas&#xA;(&#xA;  OrderNumber,&#xA;  OrderDateKey,&#xA;  ShipDateKey,&#xA;  ProductSK,&#xA;  StateSK,&#xA;  SalesOfficeSK,&#xA;  PromotionSK,&#xA;  Quantity,&#xA;  UnitPrice,&#xA;  DiscountAmount&#xA;)&#xA;SELECT&#xA;  s.OrderNumber,&#xA;  CONVERT(INT, CONVERT(CHAR(8), s.OrderDate, 112)) AS OrderDateKey,&#xA;  CASE WHEN s.ShipDate IS NULL THEN NULL&#xA;       ELSE CONVERT(INT, CONVERT(CHAR(8), s.ShipDate, 112)) END AS ShipDateKey,&#xA;&#xA;  dp.ProductSK,                                                   -- obligatorio&#xA;  COALESCE(de.StateSK,        @StateUnknownSK)        AS StateSK, -- sin NULL&#xA;  COALESCE(ofc.SalesOfficeSK, @OfficeUnknownSK)       AS SalesOfficeSK, -- sin NULL&#xA;  @PromotionSK,                                                  -- sin NULL&#xA;&#xA;  s.Quantity,&#xA;  s.UnitPrice,&#xA;  ISNULL(s.DiscountAmount, 0)&#xA;FROM stg.Ventas s&#xA;JOIN dw.DimProductos dp&#xA;  ON dp.ProductID = s.ProductID AND dp.IsCurrent = 1&#xA;LEFT JOIN dw.DimEstados de&#xA;  ON de.StateID = s.CustomerStateID AND de.IsCurrent = 1&#xA;OUTER APPLY (&#xA;  SELECT TOP 1 o.SalesOfficeSK&#xA;  FROM dw.DimOficinasVentas o&#xA;  WHERE o.StateSK = COALESCE(de.StateSK, @StateUnknownSK) AND o.IsCurrent = 1&#xA;  ORDER BY o.SalesOfficeID&#xA;) ofc&#xA;WHERE EXISTS (&#xA;  SELECT 1 FROM dw.DimFechas f&#xA;  WHERE f.DateKey = CONVERT(INT, CONVERT(CHAR(8), s.OrderDate, 112))&#xA;);&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
  </DTS:Executables>
