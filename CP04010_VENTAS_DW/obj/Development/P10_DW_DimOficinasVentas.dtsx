      DTS:LocaleID="-1"
      DTS:ObjectName="Prep_SKs_DimOficinas"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2022; © 2022 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{6CDDA0B9-EFC8-407A-8C3D-F0D92BFC3C43}"
          SQLTask:SqlStatementSource="-- Crear secuencia para SalesOfficeSK si no existe&#xA;IF OBJECT_ID('dw.SeqOfficeSK','SO') IS NULL&#xA;    CREATE SEQUENCE dw.SeqOfficeSK AS BIGINT START WITH 1 INCREMENT BY 1;&#xA;&#xA;-- Agregar default constraint para autogenerar SalesOfficeSK si no existe&#xA;IF NOT EXISTS (&#xA;  SELECT 1&#xA;  FROM sys.default_constraints dc&#xA;  JOIN sys.columns c&#xA;    ON c.object_id = OBJECT_ID('dw.DimOficinasVentas') AND c.column_id = dc.parent_column_id&#xA;  WHERE c.name = 'SalesOfficeSK'&#xA;)&#xA;BEGIN&#xA;  ALTER TABLE dw.DimOficinasVentas&#xA;  ADD CONSTRAINT DF_DimOficinasVentas_SK&#xA;    DEFAULT NEXT VALUE FOR dw.SeqOfficeSK FOR SalesOfficeSK;&#xA;END&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
