    <DTS:Executable
      DTS:refId="Package\Cargar_DimOficinas_SCD2"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Cargar_DimOficinas_SCD2"
      DTS:DTSID="{45890388-0dce-4ce5-b6e3-d78baee42adf}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Cargar_DimOficinas_SCD2"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2022; © 2022 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{6CDDA0B9-EFC8-407A-8C3D-F0D92BFC3C43}"
          SQLTask:SqlStatementSource="/* Carga SCD2 de dw.DimOficinasVentas desde stg.OficinasVentas&#xA;   Resuelve StateSK vigente desde dw.DimEstados */&#xA;DECLARE @now DATETIME2(0) = SYSUTCDATETIME();&#xA;&#xA;;WITH src AS (&#xA;  SELECT&#xA;    s.SalesOfficeID,&#xA;    s.AddressLine1, s.AddressLine2, s.City,&#xA;    d.StateSK,            -- lookup a SK vigente&#xA;    s.PostalCode, s.Telephone, s.Facsimile, s.Email&#xA;  FROM stg.OficinasVentas s&#xA;  LEFT JOIN dw.DimEstados d&#xA;    ON d.StateID = s.StateID&#xA;   AND d.IsCurrent = 1&#xA;)&#xA;MERGE dw.DimOficinasVentas AS tgt&#xA;USING src&#xA;   ON tgt.SalesOfficeID = src.SalesOfficeID&#xA;  AND tgt.IsCurrent = 1&#xA;WHEN MATCHED AND (&#xA;       ISNULL(tgt.AddressLine1,'') &lt;&gt; ISNULL(src.AddressLine1,'')&#xA;    OR ISNULL(tgt.AddressLine2,'') &lt;&gt; ISNULL(src.AddressLine2,'')&#xA;    OR ISNULL(tgt.City,'')         &lt;&gt; ISNULL(src.City,'')&#xA;    OR ISNULL(tgt.StateSK,0)       &lt;&gt; ISNULL(src.StateSK,0)&#xA;    OR ISNULL(tgt.PostalCode,'')   &lt;&gt; ISNULL(src.PostalCode,'')&#xA;    OR ISNULL(tgt.Telephone,'')    &lt;&gt; ISNULL(src.Telephone,'')&#xA;    OR ISNULL(tgt.Facsimile,'')    &lt;&gt; ISNULL(src.Facsimile,'')&#xA;    OR ISNULL(tgt.Email,'')        &lt;&gt; ISNULL(src.Email,'')&#xA;)&#xA;  THEN UPDATE&#xA;       SET tgt.IsCurrent   = 0,&#xA;           tgt.EffectiveTo = @now&#xA;WHEN NOT MATCHED BY TARGET&#xA;  THEN INSERT (&#xA;     SalesOfficeID, AddressLine1, AddressLine2, City, StateSK,&#xA;     PostalCode, Telephone, Facsimile, Email,&#xA;     EffectiveFrom, EffectiveTo, IsCurrent&#xA;  )&#xA;  VALUES (&#xA;     src.SalesOfficeID, src.AddressLine1, src.AddressLine2, src.City, src.StateSK,&#xA;     src.PostalCode, src.Telephone, src.Facsimile, src.Email,&#xA;     @now, '9999-12-31', 1&#xA;  );&#xA;&#xA;-- Insertar nueva versión para los que fueron cerrados arriba&#xA;INSERT INTO dw.DimOficinasVentas (&#xA;  SalesOfficeID, AddressLine1, AddressLine2, City, StateSK,&#xA;  PostalCode, Telephone, Facsimile, Email,&#xA;  EffectiveFrom, EffectiveTo, IsCurrent&#xA;)&#xA;SELECT&#xA;  s.SalesOfficeID, s.AddressLine1, s.AddressLine2, s.City, d.StateSK,&#xA;  s.PostalCode, s.Telephone, s.Facsimile, s.Email,&#xA;  @now, '9999-12-31', 1&#xA;FROM stg.OficinasVentas s&#xA;LEFT JOIN dw.DimEstados d&#xA;  ON d.StateID = s.StateID&#xA; AND d.IsCurrent = 1&#xA;WHERE EXISTS (&#xA;  SELECT 1&#xA;  FROM dw.DimOficinasVentas x&#xA;  WHERE x.SalesOfficeID = s.SalesOfficeID&#xA;    AND x.IsCurrent = 0&#xA;    AND x.EffectiveTo = @now&#xA;);&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
      DTS:refId="Package\Prep_SKs_DimOficinas"
