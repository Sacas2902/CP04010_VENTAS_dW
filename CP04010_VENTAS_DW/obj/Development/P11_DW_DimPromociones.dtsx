      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{D26C29E9-C5B4-4B91-B79E-FD442632182C}"
          SQLTask:SqlStatementSource="/* Carga incremental de promociones únicas desde stg.Ventas */&#xA;INSERT INTO dw.DimPromociones (PromotionName, DiscountPercent)&#xA;SELECT DISTINCT&#xA;    ISNULL(PromotionName, 'SIN PROMOCION'),&#xA;    ISNULL(DiscountPercent, 0)&#xA;FROM stg.Ventas s&#xA;WHERE NOT EXISTS (&#xA;    SELECT 1&#xA;    FROM dw.DimPromociones d&#xA;    WHERE ISNULL(d.PromotionName,'SIN PROMOCION') = ISNULL(s.PromotionName,'SIN PROMOCION')&#xA;      AND ISNULL(d.DiscountPercent,0) = ISNULL(s.DiscountPercent,0)&#xA;);&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
      DTS:refId="Package\Prep_SKs_DimPromociones"
