      DTS:ObjectName="Prep_SKs_DimPromociones"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2022; © 2022 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{D26C29E9-C5B4-4B91-B79E-FD442632182C}"
          SQLTask:SqlStatementSource="-- Crear secuencia para PromotionSK si no existe&#xA;IF OBJECT_ID('dw.SeqPromotionSK','SO') IS NULL&#xA;    CREATE SEQUENCE dw.SeqPromotionSK AS BIGINT START WITH 1 INCREMENT BY 1;&#xA;&#xA;-- Agregar default constraint si no existe&#xA;IF NOT EXISTS (&#xA;  SELECT 1&#xA;  FROM sys.default_constraints dc&#xA;  JOIN sys.columns c&#xA;    ON c.object_id = OBJECT_ID('dw.DimPromociones') AND c.column_id = dc.parent_column_id&#xA;  WHERE c.name = 'PromotionSK'&#xA;)&#xA;BEGIN&#xA;  ALTER TABLE dw.DimPromociones&#xA;  ADD CONSTRAINT DF_DimPromociones_SK&#xA;    DEFAULT NEXT VALUE FOR dw.SeqPromotionSK FOR PromotionSK;&#xA;END&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
