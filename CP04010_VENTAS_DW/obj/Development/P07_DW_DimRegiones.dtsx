      DTS:Description="Cargar_DimRegiones_SCD2"
      DTS:DTSID="{f24efed2-168e-4433-ac46-e6448226a724}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Cargar_DimRegiones_SCD2"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2022; © 2022 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{65552755-BEC1-4BD9-82CC-1C45F8C5DFBE}"
          SQLTask:SqlStatementSource="/* Carga SCD2 para dw.DimRegiones desde stg.Regiones */&#xA;DECLARE @now DATETIME2(0) = SYSUTCDATETIME();&#xA;&#xA;;WITH src AS (&#xA;  SELECT RegionID, RegionName&#xA;  FROM stg.Regiones&#xA;)&#xA;MERGE dw.DimRegiones AS tgt&#xA;USING src&#xA;  ON tgt.RegionID = src.RegionID&#xA; AND tgt.IsCurrent = 1&#xA;WHEN MATCHED AND (&#xA;     ISNULL(tgt.RegionName,'') &lt;&gt; ISNULL(src.RegionName,'')&#xA;)&#xA;  THEN UPDATE&#xA;       SET tgt.IsCurrent   = 0,&#xA;           tgt.EffectiveTo = @now&#xA;WHEN NOT MATCHED BY TARGET&#xA;  THEN INSERT (RegionID, RegionName, EffectiveFrom, EffectiveTo, IsCurrent)&#xA;       VALUES (src.RegionID, src.RegionName, @now, '9999-12-31', 1);&#xA;&#xA;-- Para los que fueron cerrados arriba, insertar su nueva versión&#xA;INSERT INTO dw.DimRegiones (RegionID, RegionName, EffectiveFrom, EffectiveTo, IsCurrent)&#xA;SELECT s.RegionID, s.RegionName, @now, '9999-12-31', 1&#xA;FROM stg.Regiones s&#xA;WHERE EXISTS (&#xA;  SELECT 1&#xA;  FROM dw.DimRegiones d&#xA;  WHERE d.RegionID = s.RegionID&#xA;    AND d.IsCurrent = 0&#xA;    AND d.EffectiveTo = @now&#xA;);&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
      DTS:refId="Package\Prep_SKs_DimRegiones"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Prep_SKs_DimRegiones"
      DTS:DTSID="{ef0dbd89-df6c-43f9-ae98-df816ecbd1c6}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Prep_SKs_DimRegiones"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2022; © 2022 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{65552755-BEC1-4BD9-82CC-1C45F8C5DFBE}"
          SQLTask:SqlStatementSource="-- Crear secuencia y default para RegionSK si no existen (idempotente)&#xA;IF OBJECT_ID('dw.SeqRegionSK','SO') IS NULL&#xA;    CREATE SEQUENCE dw.SeqRegionSK AS BIGINT START WITH 1 INCREMENT BY 1;&#xA;&#xA;IF NOT EXISTS (&#xA;    SELECT 1&#xA;    FROM sys.default_constraints dc&#xA;    JOIN sys.columns c&#xA;      ON c.object_id = OBJECT_ID('dw.DimRegiones') AND c.column_id = dc.parent_column_id&#xA;    WHERE c.name = 'RegionSK'&#xA;)&#xA;BEGIN&#xA;    ALTER TABLE dw.DimRegiones&#xA;    ADD CONSTRAINT DF_DimRegiones_SK&#xA;        DEFAULT NEXT VALUE FOR dw.SeqRegionSK FOR RegionSK;&#xA;END&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
