    <DTS:Executable
      DTS:refId="Package\Cargar_DimEstados_SCD2"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Cargar_DimEstados_SCD2&#xA;&#xA;"
      DTS:DTSID="{b74a8794-df7a-4c63-bf5e-e0fbac1403b1}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Cargar_DimEstados_SCD2"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2022; © 2022 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{E225B9B0-CBA2-423F-9532-CECC71D43076}"
          SQLTask:SqlStatementSource="/* === P08 - SCD2 DimEstados usando #src y SK explícito en INSERT === */&#xA;DECLARE @now DATETIME2(0) = SYSUTCDATETIME();&#xA;&#xA;-- 0) Asegurar la secuencia (idempotente)&#xA;IF OBJECT_ID('dw.SeqStateSK','SO') IS NULL&#xA;    CREATE SEQUENCE dw.SeqStateSK AS BIGINT START WITH 1 INCREMENT BY 1;&#xA;&#xA;-- (Opcional) Miembro DESCONOCIDA en DimRegiones si RegionSK no admite NULL&#xA;IF NOT EXISTS (SELECT 1 FROM dw.DimRegiones WHERE RegionID = -1 AND IsCurrent = 1)&#xA;BEGIN&#xA;  IF OBJECT_ID('dw.SeqRegionSK','SO') IS NULL&#xA;      CREATE SEQUENCE dw.SeqRegionSK AS BIGINT START WITH 1 INCREMENT BY 1;&#xA;&#xA;  IF NOT EXISTS (&#xA;      SELECT 1&#xA;      FROM sys.default_constraints dc&#xA;      JOIN sys.columns c&#xA;        ON c.object_id = OBJECT_ID('dw.DimRegiones') AND c.column_id = dc.parent_column_id&#xA;      WHERE c.name = 'RegionSK'&#xA;  )&#xA;  BEGIN&#xA;      ALTER TABLE dw.DimRegiones&#xA;      ADD CONSTRAINT DF_DimRegiones_SK&#xA;        DEFAULT NEXT VALUE FOR dw.SeqRegionSK FOR RegionSK;&#xA;  END;&#xA;&#xA;  INSERT INTO dw.DimRegiones (RegionID, RegionName, EffectiveFrom, EffectiveTo, IsCurrent)&#xA;  SELECT -1, N'DESCONOCIDA', SYSUTCDATETIME(), '9999-12-31', 1&#xA;  WHERE NOT EXISTS (SELECT 1 FROM dw.DimRegiones WHERE RegionID = -1 AND IsCurrent = 1);&#xA;END;&#xA;&#xA;-- 1) Fuente saneada y deduplicada&#xA;IF OBJECT_ID('tempdb..#src') IS NOT NULL DROP TABLE #src;&#xA;CREATE TABLE #src (&#xA;  StateID   INT           NOT NULL,&#xA;  StateCode NVARCHAR(50)  NOT NULL,&#xA;  StateName NVARCHAR(100) NOT NULL,&#xA;  TimeZone  NVARCHAR(100) NOT NULL,&#xA;  RegionSK  BIGINT        NULL&#xA;);&#xA;&#xA;INSERT INTO #src(StateID, StateCode, StateName, TimeZone, RegionSK)&#xA;SELECT StateID, StateCode, StateName, TimeZone, RegionSK&#xA;FROM (&#xA;  SELECT&#xA;    s.StateID,&#xA;    ISNULL(NULLIF(LTRIM(RTRIM(s.StateCode)), ''), 'SIN CODIGO') AS StateCode,&#xA;    ISNULL(NULLIF(LTRIM(RTRIM(s.StateName)), ''), 'SIN NOMBRE')  AS StateName,&#xA;    ISNULL(NULLIF(LTRIM(RTRIM(s.TimeZone)), ''),  'SIN ZONA')    AS TimeZone,&#xA;    COALESCE(r.RegionSK,&#xA;             (SELECT TOP 1 RegionSK FROM dw.DimRegiones WHERE RegionID=-1 AND IsCurrent=1)) AS RegionSK,&#xA;    ROW_NUMBER() OVER (PARTITION BY s.StateID ORDER BY s.StateID) AS rn&#xA;  FROM stg.Estados s&#xA;  LEFT JOIN dw.DimRegiones r&#xA;    ON r.RegionID = s.RegionID AND r.IsCurrent = 1&#xA;) x&#xA;WHERE x.rn = 1;&#xA;&#xA;-- 2) Cerrar versión vigente cuando cambie algo&#xA;UPDATE d&#xA;   SET d.IsCurrent = 0,&#xA;       d.EffectiveTo = @now&#xA;FROM dw.DimEstados d&#xA;JOIN #src s&#xA;  ON d.StateID = s.StateID&#xA; AND d.IsCurrent = 1&#xA;WHERE ISNULL(d.StateCode,'') &lt;&gt; ISNULL(s.StateCode,'')&#xA;   OR ISNULL(d.StateName,'') &lt;&gt; ISNULL(s.StateName,'')&#xA;   OR ISNULL(d.TimeZone,'')  &lt;&gt; ISNULL(s.TimeZone,'')&#xA;   OR ISNULL(d.RegionSK,0)   &lt;&gt; ISNULL(s.RegionSK,0);&#xA;&#xA;-- 3) Insertar NUEVOS (con StateSK explícito desde la secuencia)&#xA;INSERT INTO dw.DimEstados (&#xA;  StateSK, StateID, StateCode, StateName, TimeZone, RegionSK,&#xA;  EffectiveFrom, EffectiveTo, IsCurrent&#xA;)&#xA;SELECT&#xA;  NEXT VALUE FOR dw.SeqStateSK,&#xA;  s.StateID, s.StateCode, s.StateName, s.TimeZone, s.RegionSK,&#xA;  @now, '9999-12-31', 1&#xA;FROM #src s&#xA;LEFT JOIN dw.DimEstados d&#xA;  ON d.StateID = s.StateID AND d.IsCurrent = 1&#xA;WHERE d.StateID IS NULL;&#xA;&#xA;-- 4) Insertar NUEVA VERSIÓN (también con StateSK explícito)&#xA;INSERT INTO dw.DimEstados (&#xA;  StateSK, StateID, StateCode, StateName, TimeZone, RegionSK,&#xA;  EffectiveFrom, EffectiveTo, IsCurrent&#xA;)&#xA;SELECT&#xA;  NEXT VALUE FOR dw.SeqStateSK,&#xA;  s.StateID, s.StateCode, s.StateName, s.TimeZone, s.RegionSK,&#xA;  @now, '9999-12-31', 1&#xA;FROM #src s&#xA;JOIN dw.DimEstados d&#xA;  ON d.StateID = s.StateID&#xA; AND d.IsCurrent = 0&#xA; AND d.EffectiveTo = @now;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
      DTS:refId="Package\Prep_SKs_DimEstados"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Prep_SKs_DimEstados"
      DTS:DTSID="{63699b3d-9a9d-47a4-84b9-455d0b2ee46c}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Prep_SKs_DimEstados"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2022; © 2022 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{E225B9B0-CBA2-423F-9532-CECC71D43076}"
          SQLTask:SqlStatementSource="/* PREP Estados: secuencia, default y backfill de SK nulos */&#xA;IF OBJECT_ID('dw.SeqStateSK','SO') IS NULL&#xA;    CREATE SEQUENCE dw.SeqStateSK AS BIGINT START WITH 1 INCREMENT BY 1;&#xA;&#xA;IF NOT EXISTS (&#xA;  SELECT 1&#xA;  FROM sys.default_constraints dc&#xA;  JOIN sys.columns c&#xA;    ON c.object_id = OBJECT_ID('dw.DimEstados') AND c.column_id = dc.parent_column_id&#xA;  WHERE c.name = 'StateSK'&#xA;)&#xA;BEGIN&#xA;  ALTER TABLE dw.DimEstados&#xA;  ADD CONSTRAINT DF_DimEstados_SK&#xA;    DEFAULT NEXT VALUE FOR dw.SeqStateSK FOR StateSK;&#xA;END&#xA;&#xA;/* Si por intentos anteriores quedó alguna fila con StateSK NULL, la rellenamos */&#xA;UPDATE d&#xA;SET StateSK = NEXT VALUE FOR dw.SeqStateSK&#xA;FROM dw.DimEstados d&#xA;WHERE d.StateSK IS NULL;&#xA;&#xA;/* (Opcional) crear miembro desconocido en Regiones si RegionSK no admite NULL */&#xA;IF NOT EXISTS (SELECT 1 FROM dw.DimRegiones WHERE RegionID = -1 AND IsCurrent = 1)&#xA;BEGIN&#xA;  IF OBJECT_ID('dw.SeqRegionSK','SO') IS NULL&#xA;      CREATE SEQUENCE dw.SeqRegionSK AS BIGINT START WITH 1 INCREMENT BY 1;&#xA;&#xA;  IF NOT EXISTS (&#xA;      SELECT 1&#xA;      FROM sys.default_constraints dc&#xA;      JOIN sys.columns c&#xA;        ON c.object_id = OBJECT_ID('dw.DimRegiones') AND c.column_id = dc.parent_column_id&#xA;      WHERE c.name = 'RegionSK'&#xA;  )&#xA;  BEGIN&#xA;      ALTER TABLE dw.DimRegiones&#xA;      ADD CONSTRAINT DF_DimRegiones_SK&#xA;        DEFAULT NEXT VALUE FOR dw.SeqRegionSK FOR RegionSK;&#xA;  END&#xA;&#xA;  INSERT INTO dw.DimRegiones (RegionID, RegionName, EffectiveFrom, EffectiveTo, IsCurrent)&#xA;  SELECT -1, N'DESCONOCIDA', SYSUTCDATETIME(), '9999-12-31', 1&#xA;  WHERE NOT EXISTS (SELECT 1 FROM dw.DimRegiones WHERE RegionID = -1 AND IsCurrent = 1);&#xA;END&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
  </DTS:Executables>
