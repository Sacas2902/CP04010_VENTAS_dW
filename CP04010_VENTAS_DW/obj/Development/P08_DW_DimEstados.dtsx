      DTS:Description="Cargar_DimEstados_SCD2&#xA;&#xA;"
      DTS:DTSID="{b74a8794-df7a-4c63-bf5e-e0fbac1403b1}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Cargar_DimEstados_SCD2"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2022; © 2022 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{0DF2292B-9815-4164-85C2-A8ECA5A909BE}"
          SQLTask:SqlStatementSource="/* Carga SCD2 para dw.DimEstados desde stg.Estados&#xA;   Hace lookup del RegionSK vigente en DimRegiones */&#xA;DECLARE @now DATETIME2(0) = SYSUTCDATETIME();&#xA;&#xA;;WITH src AS (&#xA;  SELECT&#xA;    s.StateID, s.StateCode, s.StateName, s.TimeZone,&#xA;    r.RegionSK&#xA;  FROM stg.Estados s&#xA;  LEFT JOIN dw.DimRegiones r&#xA;    ON r.RegionID = s.RegionID&#xA;   AND r.IsCurrent = 1&#xA;)&#xA;MERGE dw.DimEstados AS tgt&#xA;USING src&#xA;  ON tgt.StateID = src.StateID&#xA; AND tgt.IsCurrent = 1&#xA;WHEN MATCHED AND (&#xA;     ISNULL(tgt.StateCode,'')  &lt;&gt; ISNULL(src.StateCode,'')&#xA;  OR ISNULL(tgt.StateName,'')  &lt;&gt; ISNULL(src.StateName,'')&#xA;  OR ISNULL(tgt.TimeZone,'')   &lt;&gt; ISNULL(src.TimeZone,'')&#xA;  OR ISNULL(tgt.RegionSK,0)    &lt;&gt; ISNULL(src.RegionSK,0)&#xA;)&#xA;  THEN UPDATE&#xA;       SET tgt.IsCurrent   = 0,&#xA;           tgt.EffectiveTo = @now&#xA;WHEN NOT MATCHED BY TARGET&#xA;  THEN INSERT (StateID, StateCode, StateName, TimeZone, RegionSK,&#xA;               EffectiveFrom, EffectiveTo, IsCurrent)&#xA;       VALUES (src.StateID, src.StateCode, src.StateName, src.TimeZone, src.RegionSK,&#xA;               @now, '9999-12-31', 1);&#xA;&#xA;-- Insertar nuevas versiones para los que se cerraron arriba&#xA;INSERT INTO dw.DimEstados&#xA;  (StateID, StateCode, StateName, TimeZone, RegionSK,&#xA;   EffectiveFrom, EffectiveTo, IsCurrent)&#xA;SELECT&#xA;  s.StateID, s.StateCode, s.StateName, s.TimeZone,&#xA;  r.RegionSK,&#xA;  @now, '9999-12-31', 1&#xA;FROM stg.Estados s&#xA;LEFT JOIN dw.DimRegiones r&#xA;  ON r.RegionID = s.RegionID&#xA; AND r.IsCurrent = 1&#xA;WHERE EXISTS (&#xA;  SELECT 1&#xA;  FROM dw.DimEstados d&#xA;  WHERE d.StateID = s.StateID&#xA;    AND d.IsCurrent = 0&#xA;    AND d.EffectiveTo = @now&#xA;);&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
      DTS:refId="Package\Prep_SKs_DimEstados"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Prep_SKs_DimEstados"
