      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{4662B4FD-23F6-4EA7-B8CB-363EBB6B3C0E}"
          SQLTask:SqlStatementSource="/* Truncar y cargar stg.Ventas desde TailspinToys2020-US.dbo.Sales */&#xA;TRUNCATE TABLE stg.Ventas;&#xA;&#xA;INSERT INTO stg.Ventas (&#xA;  OrderNumber, OrderDate, ShipDate, CustomerStateID,&#xA;  ProductID, Quantity, UnitPrice, DiscountAmount, PromotionCode&#xA;)&#xA;SELECT&#xA;  s.OrderNumber, s.OrderDate, s.ShipDate, s.CustomerStateID,&#xA;  s.ProductID, s.Quantity, s.UnitPrice, s.DiscountAmount, s.PromotionCode&#xA;FROM [TailspinToys2020-US].dbo.Sales AS s;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
      DTS:refId="Package\Validadcion"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="validacion "
      DTS:DTSID="{CF59D3E1-5874-411C-8335-69422596C4EE}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Validadcion"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2022; © 2022 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{4662B4FD-23F6-4EA7-B8CB-363EBB6B3C0E}"
          SQLTask:SqlStatementSource="DECLARE @minOrder date = (SELECT MIN(CAST(OrderDate AS date)) FROM stg.Ventas);&#xA;DECLARE @maxDates date = (&#xA;  SELECT MAX(x.d)&#xA;  FROM (SELECT MAX(CAST(OrderDate AS date)) d FROM stg.Ventas&#xA;        UNION ALL&#xA;        SELECT MAX(CAST(ShipDate  AS date)) d FROM stg.Ventas) x&#xA;);&#xA;&#xA;IF @minOrder IS NULL OR @maxDates IS NULL&#xA;BEGIN&#xA;  -- staging sin fechas: no hacemos nada y salimos sin error&#xA;  RETURN;&#xA;END;&#xA;&#xA;DECLARE @StartDate date = DATEADD(YEAR, -3, @minOrder);&#xA;DECLARE @EndDate   date = DATEADD(YEAR,  3, @maxDates);&#xA;&#xA;;WITH d AS (&#xA;  SELECT @StartDate AS [Date]&#xA;  UNION ALL&#xA;  SELECT DATEADD(DAY,1,[Date]) FROM d WHERE [Date] &lt; @EndDate&#xA;)&#xA;INSERT INTO dw.DimFechas&#xA;  (DateKey,[Date],[Year],[Quarter],[Month],MonthName,[Day],DayOfWeek,DayName,IsWeekend)&#xA;SELECT&#xA;  CONVERT(int, CONVERT(char(8), d.[Date], 112)),&#xA;  d.[Date],&#xA;  DATEPART(YEAR,d.[Date]),&#xA;  DATEPART(QUARTER,d.[Date]),&#xA;  DATEPART(MONTH,d.[Date]),&#xA;  DATENAME(MONTH,d.[Date]),&#xA;  DATEPART(DAY,d.[Date]),&#xA;  DATEPART(WEEKDAY,d.[Date]),&#xA;  DATENAME(WEEKDAY,d.[Date]),&#xA;  CASE WHEN DATENAME(WEEKDAY,d.[Date]) IN (N'Saturday', N'Sunday') THEN 1 ELSE 0 END&#xA;FROM d&#xA;WHERE NOT EXISTS (&#xA;  SELECT 1&#xA;  FROM dw.DimFechas f&#xA;  WHERE f.DateKey = CONVERT(int, CONVERT(char(8), d.[Date], 112))&#xA;)&#xA;OPTION (MAXRECURSION 0);&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
