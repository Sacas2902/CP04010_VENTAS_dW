          SQLTask:Connection="{B74DB0C3-A19C-462E-8162-2EEAD4967E08}"
          SQLTask:SqlStatementSource="DECLARE @StartDate DATETIME, @EndDate DATETIME;&#xA;SET @StartDate = CAST(? AS DATETIME);&#xA;SET @EndDate   = CAST(? AS DATETIME);&#xA;&#xA;;WITH d AS (&#xA;  SELECT CAST(@StartDate AS DATE) AS [Date]&#xA;  UNION ALL&#xA;  SELECT DATEADD(DAY,1,[Date]) FROM d WHERE [Date] &lt; CAST(@EndDate AS DATE)&#xA;)&#xA;INSERT INTO dw.DimFechas(DateKey,[Date],[Year],[Quarter],[Month],MonthName,[Day],DayOfWeek,DayName,IsWeekend)&#xA;SELECT&#xA;  CONVERT(INT, CONVERT(CHAR(8), [Date], 112)),&#xA;  [Date],&#xA;  DATEPART(YEAR,[Date]),&#xA;  DATEPART(QUARTER,[Date]),&#xA;  DATEPART(MONTH,[Date]),&#xA;  DATENAME(MONTH,[Date]),&#xA;  DATEPART(DAY,[Date]),&#xA;  DATEPART(WEEKDAY,[Date]),&#xA;  DATENAME(WEEKDAY,[Date]),&#xA;  CASE WHEN DATENAME(WEEKDAY,[Date]) IN (N'Saturday',N'Sunday') THEN 1 ELSE 0 END&#xA;FROM d&#xA;WHERE NOT EXISTS (SELECT 1 FROM dw.DimFechas f WHERE f.DateKey = CONVERT(INT, CONVERT(CHAR(8), d.[Date], 112)))&#xA;OPTION (MAXRECURSION 0);&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
